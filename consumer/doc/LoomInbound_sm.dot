digraph LoomInbound {

    node
        [shape=Mrecord width=1.5];

    subgraph cluster_LoomInboundFSM {

        label="LoomInboundFSM";

        //
        // States (Nodes)
        //

        "LoomInboundFSM::Suspended"
            [label="{Suspended}"];

        "%start"
            [label="" shape=circle style=filled fillcolor=black width=0.25];

        //
        // Transitions (Edges)
        //

        "%start" -> "LoomInboundFSM::Suspended"
    }

}
